<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/login_ui_bg_color"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".main.navigation_fragment.d.notification.SoundEffectFragment">


        <ImageView
            android:id="@+id/ivArrowBack1"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_margin="20dp"
            android:background="@drawable/arrow_back"
            android:backgroundTint="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/montserrat_extra_bold"
            android:gravity="center"
            android:text="@string/notification_"
            android:layout_marginStart="40dp"
            android:textColor="@color/text_default_light"
            android:textSize="30dp"
            app:layout_constraintStart_toEndOf="@+id/ivArrowBack1"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:id="@+id/llnotification1"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/button_shape_graffit_r12"
            android:layout_marginHorizontal="15dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/montserrat_extra_bold"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:layout_marginStart="10dp"
                    android:text="@string/sound_effects"
                    android:textColor="@color/text_default_light"
                    android:textSize="20dp" />

                <Switch
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginEnd="10dp"
                    android:checked="true"
                    android:paddingTop="10dp"
                    android:thumbTint="@drawable/switch_thumb_tint"
                    android:trackTint="@drawable/switch_track_tint" />
        </LinearLayout>


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="#313747"
            app:cardCornerRadius="20dp"
            android:layout_marginTop="15dp"
            android:layout_marginHorizontal="15dp"
            app:layout_constraintTop_toBottomOf="@+id/llnotification1">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                        <LinearLayout
                            android:id="@+id/linearLayout10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="20dp"
                            android:orientation="horizontal"
                            android:paddingVertical="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                                <TextView
                                    android:id="@+id/textView8"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginStart="15dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/montserrat_extra_bold"
                                    android:text="@string/sound_1"
                                    android:textColor="@color/white"
                                    android:textSize="22dp" />

                                <ImageView
                                    android:id="@+id/ivTrackOn1"
                                    android:layout_width="19dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginEnd="10dp"
                                    android:src="@drawable/ic_icon_general_check"
                                    android:visibility="visible" />

                        </LinearLayout>

                        <View
                            android:id="@+id/view4"
                            android:layout_width="match_parent"
                            android:layout_height="3dp"
                            android:layout_marginHorizontal="30dp"
                            android:background="#BC6E727C"
                            android:elevation="10dp"
                            app:layout_constraintTop_toBottomOf="@id/linearLayout10" />

                        <LinearLayout
                            android:id="@+id/linearLayout11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="20dp"
                            android:orientation="horizontal"
                            android:paddingVertical="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view4">

                                <TextView
                                    android:id="@+id/textView12"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginStart="15dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/montserrat_extra_bold"
                                    android:text="@string/sound_2"
                                    android:textColor="@color/white"
                                    android:textSize="22dp" />

                                <ImageView
                                    android:id="@+id/ivTrackOn2"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginEnd="10dp"
                                    android:src="@drawable/ic_icon_general_check"
                                    android:visibility="gone" />

                        </LinearLayout>

                        <View
                            android:id="@+id/view5"
                            android:layout_width="match_parent"
                            android:layout_height="3dp"
                            android:layout_marginHorizontal="30dp"
                            android:background="#BC6E727C"
                            android:elevation="10dp"
                            app:layout_constraintTop_toBottomOf="@id/linearLayout11" />

                        <LinearLayout
                            android:id="@+id/linearLayout12"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="20dp"
                            android:orientation="horizontal"
                            android:paddingVertical="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view5">

                                <TextView
                                    android:id="@+id/textView11"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginStart="15dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/montserrat_extra_bold"
                                    android:text="@string/sound_3"
                                    android:textColor="@color/white"
                                    android:textSize="22dp" />

                                <ImageView
                                    android:id="@+id/ivTrackOn3"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginEnd="10dp"
                                    android:src="@drawable/ic_icon_general_check"
                                    android:visibility="gone" />
                        </LinearLayout>

                        <View
                            android:id="@+id/view6"
                            android:layout_width="match_parent"
                            android:layout_height="3dp"
                            android:layout_marginHorizontal="30dp"
                            android:background="#BC6E727C"
                            android:elevation="10dp"
                            app:layout_constraintTop_toBottomOf="@id/linearLayout12" />

                        <LinearLayout
                            android:id="@+id/linearLayout13"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="20dp"
                            android:orientation="horizontal"
                            android:paddingVertical="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/view6">

                                <TextView
                                    android:id="@+id/textView13"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginStart="15dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/montserrat_extra_bold"
                                    android:text="@string/sound_4"
                                    android:textColor="@color/white"
                                    android:textSize="22dp" />

                                <ImageView
                                    android:id="@+id/ivTrackOn4"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginEnd="10dp"
                                    android:src="@drawable/ic_icon_general_check"
                                    android:visibility="gone" />

                        </LinearLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>